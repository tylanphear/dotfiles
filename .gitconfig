[user]
	email = tylanphear@gmail.com
        name = Tyler Lanphear

[color]
        ui = auto

[diff]
	tool = vimdiff
[alias]
	lo = log --oneline
        cna = commit --no-edit --amend
	conflicts = diff --name-only --diff-filter=U
        pushfl = push --force-with-lease
        wip = commit -am'!WIP!'
[status]
	showuntrackedfiles = no
[core]
	editor = nvim
[push]
	default = current
[http]
	sslVerify = false
